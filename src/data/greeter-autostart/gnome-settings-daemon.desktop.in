[Desktop Entry]
Type=Application
_Name=GNOME Settings Daemon
TryExec=/usr/local/libexec/gnome-settings-daemon
Exec=/usr/local/libexec/gnome-settings-daemon --gconf-prefix=/apps/gdm/simple-greeter/settings-manager-plugins
OnlyShowIn=GNOME;
X-GNOME-Autostart-Phase=Initialization
X-GNOME-Autostart-Notify=true
X-GNOME-AutoRestart=true
